{
  "name": "wedding-package-maker",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "10.14.2",
    "yarn": "1.12.3",
    "npm": "6.4.1"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "express": "^4.16.4",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "2.1.1"
  },
  "scripts": {
    "dev:css:compile": "cross-env node-sass src/styles/scss/main.scss src/styles/css/style.css -w",
    "prod:css:compile": "cross-env node-sass src/styles/scss/main.scss src/styles/css/style.css",
    "start:dev": "npm run dev:css:compile | npm run dev:proxy:server | npm run client:react",
    "client:react": "react-scripts start",
    "build": "cross-env react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prod:server": "babel-node --presets es2015 server.js",
    "dev:proxy:server": "cross-env PORT=9000 NODE_ENV=development npm run prod:server",
    "prod:run": "npm run build && npm run prod:server",
    "postinstall": "npm run prod:css:compile && npm run build",
    "start": "npm run prod:server"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.2.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.9"
  },
  "proxy": "http://localhost:9000",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
