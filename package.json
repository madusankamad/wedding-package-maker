{
  "name": "wedding-package-maker",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "axios": "^0.18.0",
    "bootstrap": "^4.1.3",
    "express": "^4.16.4",
    "react": "^16.3.2",
    "react-dom": "^16.3.2",
    "react-scripts": "1.1.4",
    "redux": "^4.0.1",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0"
  },
  "scripts": {
    "scss-compile": "cross-env node-sass src/styles/scss/main.scss src/styles/css/style.css -w",
    "scss-compile-prod": "cross-env node-sass src/styles/scss/main.scss src/styles/css/style.css",
    "start:dev": "npm run scss-compile | npm run dev:proxy:server | npm run start:react",
    "start:react": "react-scripts start",
    "build": "cross-env react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject",
    "prod:server:build": "webpack --config webpack.server.config --mode production",
    "prod:server": "npm run prod:server:build && node server.bundle.js",
    "dev:proxy:server": "cross-env PORT=9000 NODE_ENV=development npm run prod:server",
    "build:prod:server": "npm run build && npm run prod:server",
    "postinstall": "npm run scss-compile-prod && npm run build && npm run prod:server:build",
    "start": "node server.bundle.js"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.1.5",
    "@babel/node": "^7.2.2",
    "@babel/plugin-proposal-object-rest-spread": "^7.2.0",
    "@babel/plugin-transform-modules-commonjs": "^7.2.0",
    "@babel/preset-env": "^7.1.5",
    "@babel/preset-stage-0": "^7.0.0",
    "@babel/preset-stage-2": "^7.0.0",
    "babel-loader": "^8.0.4",
    "babel-preset-es2015": "^6.24.1",
    "cross-env": "^5.2.0",
    "node-sass": "^4.9.0",
    "nodemon": "^1.18.9",
    "webpack": "^4.28.0",
    "webpack-cli": "^3.1.2",
    "webpack-node-externals": "^1.7.2"
  },
  "proxy": "http://localhost:9000"
}
